#!/bin/bash
realpath=`readlink -fn $0`
export RPM_USRLIBRPM=/home/daniel/Dropbox/Hobby/Zybo_Xilinx_Xilibus/Yocto/poky/build/tmp/sysroots/x86_64-linux/usr/lib/rpm RPM_ETCRPM=/home/daniel/Dropbox/Hobby/Zybo_Xilinx_Xilibus/Yocto/poky/build/tmp/sysroots/x86_64-linux/etc/rpm RPM_LOCALEDIRRPM=/home/daniel/Dropbox/Hobby/Zybo_Xilinx_Xilibus/Yocto/poky/build/tmp/sysroots/x86_64-linux/usr/share/locale
exec -a `dirname $realpath`/createrepo `dirname $realpath`/createrepo.real "$@"
